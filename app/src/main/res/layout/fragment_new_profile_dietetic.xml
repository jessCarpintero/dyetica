<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/frame_layout_new_id"
    tools:context="com.dyetica.app.fragments.DieteticProfileFragment">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/dietetic_profile_title"
        android:layout_gravity="center_horizontal|bottom" />

    <TextView
        android:id="@+id/dietetic_profile_text"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical|center_horizontal"
         />

    <Button
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:text="Crear perfil"
        android:id="@+id/buttonProfileDietetic"
        android:layout_gravity="center_horizontal"
        android:theme="@style/ButtonApp"/>
</FrameLayout>
